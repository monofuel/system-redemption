    "@types/convict": "^4.2.1",
    "@types/dat.gui": "^0.7.2",
    "@types/express": "^4.16.0",
    "@types/express-ws": "^3.0.0",
    "@types/jest": "^26.0.19",
    "@types/jsdom": "^16.2.5",
    "@types/jszip": "^3.1.4",
    "@types/lodash": "^4.14.119",
    "@types/ncp": "^2.0.1",
    "@types/node": "^14.14.16",
    "@types/three": "^0.93.13",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "bulma": "^0.7.4",
    "canvas": "^2.6.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^4.1.0",
    "convict": "^5.1.0",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "dat.gui": "^0.7.3",
    "eslint": "^7.18.0",
