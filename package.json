{
  "name": "system-redemption",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "jest": {
    "projects": [
      "jest.config.js",
      "src/elements/jest.config.js"
    ]
  },
  "scripts": {
    "server": "ts-node src/server.ts",
    "server:watch": "nodemon build/server/server.js --baseDir .",
    "build": "concurrently 'yarn build:client' 'yarn build:server'",
    "build:client": "webpack --debug",
    "build:server": "tsc",
    "watch": "concurrently 'yarn watch:client' 'yarn watch:server'",
    "watch:client": "webpack --debug -w",
    "watch:server": "tsc -w",
    "prepare:client": "ts-node ./build-src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "NODE_ENV=test jest --detectOpenHandles --projects ./ ./src/elements --maxWorkers=2",
    "coverage": "NODE_ENV=test jest  --projects ./ ./src/elements --maxWorkers=2 --coverage --coverageReporters=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.japura.net/monofuel/system-redemption"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
